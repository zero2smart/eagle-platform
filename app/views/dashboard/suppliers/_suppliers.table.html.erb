<div class="table-responsive border-bottom" data-toggle="lists" data-lists-values='["js-lists-values-employee-name"]'>
  <table class="table mb-0 thead-border-top-0">
    <thead>
      <tr>
        <th>Supplier</th>
        <th>State</th>
        <th>City</th>
        <th>Phone</th>
        <th></th>
      </tr>
    </thead>
    <tbody class="list" id="blah">
      <% ["White Rock", "Vulcan Materials", "SDI"].each do |name| %>
        <tr>
          <td><strong><%= link_to name.to_s, dashboard_supplier_path(1)  %></strong></td>
          <td><%= Faker::Address.state %></td>
          <td><%= Faker::Address.city %></td>
          <td><%= Faker::PhoneNumber.phone_number %></td>
          <td></td>
          <td class="text-right dropdown">
            <a href="" class="text-muted dropdown-toggle" data-toggle="dropdown" data-caret="false"><i class="material-icons">more_vert</i></a>
            <div class="dropdown-menu dropdown-menu-right">
              <a class="dropdown-item" href="#">Edit</a>
              <a class="dropdown-item" href="#">Delete</a>
            </div>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>