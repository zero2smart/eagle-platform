<!DOCTYPE html>
<html>
  <head>
    <title>Eagle Carriers</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="theme-color" content="#000000">
    <link rel="manifest" href="/packs/manifest.json">
    <%= stylesheet_link_tag 'dispatch', media: 'all' %>
    <%= javascript_include_tag 'dispatch' %>
    <%= javascript_pack_tag 'dispatch-bundle' %>
  </head>
  <body>
    <div className="root d-flex">
      <div className="content-wrapper">
        <%= render 'dispatch/navbar' %>
        <%#= render 'dispatch/header' %>
        <%= yield %>
      </div>
    </div>
  </body>
</html>
